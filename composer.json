{
    "name": "stidges/laravel-fk-migration",
    "description": "Helpful base migration for creating all your foreign key without worrying about the order of your migrations.",
    "version": "1.1.0",
    "homepage": "https://github.com/stidges/laravel-fk-migration",
    "authors": [
        {
            "name": "Stidges",
            "email": "info@stidges.com",
            "homepage": "http://twitter.com/stidges"
        }
    ],
    "keywords": [
        "db",
        "laravel",
        "migrations"
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/database": "~4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "Stidges\\LaravelFkMigration\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stidges\\LaravelFkMigration\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable",
    "license": "MIT"
}
